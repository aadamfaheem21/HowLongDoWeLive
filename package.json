{
  "name": "howlongdowelive",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "precompile": "npm run rimraf build/* && npm run rimraf dist/*",
    "compile:css": "./node_modules/less/bin/lessc ./src/LESS/styles.less > ./build/css/styles.css",
    "compile:html": "node generator.js",
    "compile": "npm run compile:css && npm run compile:html",
    "lint": "jshint ./build/js/*.js",
    "build": "npm run compile && npm run lint",
    "squish": "mkdir dev && minify generator.js",
    "build:start": "open http://127.0.0.1:8080 && node ./node_modules/http-server/bin/http-server ./build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aadamfaheem21/HowLongDoWeLive.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aadamfaheem21/HowLongDoWeLive/issues"
  },
  "homepage": "https://github.com/aadamfaheem21/HowLongDoWeLive#readme",
  "dependencies": {
    "d3": "^6.3.1",
    "ejs": "^3.1.5",
    "fs": "0.0.1-security",
    "jest": "^26.6.3",
    "jshint": "^2.12.0",
    "less": "^3.12.2",
    "rimraf": "^3.0.2",
    "http-server": "^0.12.3"
  },
  "jshintConfig": {
    "curly": true,
    "eqeqeq": true,
    "undef": true,
    "unused": true,
    "varstmt": true
  },
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/test/**/*.test.js"
    ]
  },
  "devDependencies": {}
}
